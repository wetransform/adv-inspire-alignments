<sld:StyledLayerDescriptor xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd">
  <sld:NamedLayer>
    <se:Name>CP.CadastralParcel</se:Name>
    <se:Description>
		<se:Title>Cadastral Boundary only</se:Title>
	</se:Description>
    <sld:UserStyle>
      <se:Name>CP.CadastralParcel.BoundariesOnly</se:Name>
      <sld:IsDefault>0</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:cp="http://inspire.ec.europa.eu/schemas/cp/4.0">
        <se:Description>
          <se:Title>Cadastral Parcel - Boundaries Only Default Style</se:Title>
          <se:Abstract>Parcel outline is rendered as a 1 pixel black (#000000) line.</se:Abstract>
        </se:Description>
        <se:FeatureTypeName>cp:CadastralParcel</se:FeatureTypeName>
        <se:Rule>
        <se:Description>
          <se:Title>cadastral parcel - boundaries only</se:Title>
        </se:Description>
          <se:MinScaleDenominator>1</se:MinScaleDenominator>
          <se:MaxScaleDenominator>40000</se:MaxScaleDenominator>
          <se:PolygonSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>cp:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#000000</se:SvgParameter>
              <se:SvgParameter name="stroke-width">1</se:SvgParameter>
            </se:Stroke>
          </se:PolygonSymbolizer>
        </se:Rule>
      </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>
